{
  "name": "@bugfixes/react-utils",
  "version": "0.1.1",
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "main": "./lib/main/index.js",
  "module": "./lib/module/index.js",
